<?xml version="1.0"?>

<launch>
	<!-- roscore automatically started by roslaunch -->
	
	<!-- start arduino -->
	<node pkg="rosserial_python" type="serial_node.py" name="rosserial_controller" output="screen">
		<param name="~port" value="/dev/ttyUSB0" />
		<param name="~baud" value="57600" />
	</node>
</launch>
